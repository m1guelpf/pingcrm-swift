{
	"compilerOptions": {
		"allowJs": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"strict": true,
		"isolatedModules": true,
		"target": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["resources/js/*"]
		},
		"types": ["node", "vite/client"]
	},
	"include": ["resources/js/**/*.ts", "resources/js/**/*.tsx", "resources/js/**/*.d.ts"]
}
